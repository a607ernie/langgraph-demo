---
config:
  theme: default
  flowchart:
    curve: linear
---
graph TD;
	__start__([<p>__start__</p>]):::first
	planner(planner)
	agent(agent)
	replan(replan)
	__end__([<p>__end__</p>]):::last
	__start__ --> planner;
	agent --> replan;
	planner --> agent;
	replan -.-> __end__;
	replan -.-> agent;
	classDef default fill:#f2f0ff,line-height:1.2
	classDef first fill-opacity:0
	classDef last fill:#bfb6fc
